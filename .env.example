# ===========================
# ENVIRONMENT SETTINGS
# ===========================
# Possible values: sandbox, production
APP_ENV=sandbox

# ===========================
# DATABASE SETTINGS
# ===========================
DB_HOST=127.0.0.1
DB_NAME=db_agoda
DB_USER=root
DB_PASS=

# ===========================
# AGODA API SETTINGS
# ===========================
AGODA_API_KEY_SANDBOX=YOUR_SANDBOX_API_KEY
AGODA_SECRET_SANDBOX=YOUR_SANDBOX_SECRET

AGODA_API_KEY_PRODUCTION=YOUR_PRODUCTION_API_KEY
AGODA_SECRET_PRODUCTION=YOUR_PRODUCTION_SECRET

# ===========================
# LOGGING
# ===========================
LOG_FILE=storage.log

# ===========================
# QUEUE SETTINGS
# ===========================
QUEUE_RETRY_COUNT=3
QUEUE_SLEEP_SECONDS=5

# ===========================
# CRON / WORKER SETTINGS
# ===========================
CRON_PULL_RESERVATIONS_INTERVAL_MINUTES=5
CRON_SYNC_RATES_INTERVAL_MINUTES=15
CRON_SYNC_INVENTORY_INTERVAL_MINUTES=15